executable = /bin/bash
arguments = train_cluster.sh
error = cvae_train.job.err
output = train.job.out
log = train.job.log
request_memory = 10000
request_gpus = 1
request_cpus = 1
MaxTime = 648000
periodic_hold = (CurrentTime - JobCurrentStartDate) >= $(MaxTime)
periodic_hold_reason = "Job runtime exceeded"
queue 1